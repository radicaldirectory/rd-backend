{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "PostContent": {
      "type": "text"
    },
    "Event": {
      "model": "events",
      "via": "Posts"
    },
    "Group": {
      "via": "Posts",
      "model": "groups"
    },
    "User": {
      "via": "PostsAuthored",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
